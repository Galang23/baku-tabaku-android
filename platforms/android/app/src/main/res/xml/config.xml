<?xml version='1.0' encoding='utf-8'?>
<widget id="com.caksono.bakutabaku" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <allow-navigation href="http://localhost/*" />
    <allow-navigation href="https://localhost/*" />
    <allow-navigation href="ionic://*" />
    <feature name="IonicWebView">
        <param name="android-package" value="com.ionicframework.cordova.webview.IonicWebView" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="CDVOrientation">
        <param name="android-package" value="cordova.plugins.screenorientation.CDVOrientation" />
    </feature>
    <name>Baku Tabaku</name>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="www/android-icon-36.png" />
    <icon density="mdpi" src="www/android-icon-48.png" />
    <icon density="hdpi" src="www/android-icon-72.png" />
    <icon density="xhdpi" src="www/android-icon-96.png" />
    <icon density="xxhdpi" src="www/android-icon-144.png" />
    <icon density="xxxhdpi" src="www/android-icon-192.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="webView" value="com.ionicframework.cordova.webview.IonicWebViewEngine" />
    <preference name="AndroidXEnabled" value="true" />
    <preference name="BackgroundColor" value="0xff000000" />
    <preference name="Fullscreen" value="true" />
    <preference name="phonegap-version" value="cli-9.0.0" />
</widget>
